{
  "name": "ngfire",
  "version": "0.0.1",
  "description": "Minimal Angular wrapper around the Firebase JS SDK",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./app": {
      "types": "./dist/app/index.d.ts",
      "default": "./dist/app/index.js"
    },
    "./analytics": {
      "types": "./dist/analytics/index.d.ts",
      "default": "./dist/analytics/index.js"
    },
    "./auth": {
      "types": "./dist/auth/index.d.ts",
      "default": "./dist/auth/index.js"
    },
    "./firestore": {
      "types": "./dist/firestore/index.d.ts",
      "default": "./dist/firestore/index.js"
    },
    "./functions": {
      "types": "./dist/functions/index.d.ts",
      "default": "./dist/functions/index.js"
    },
    "./emulators": {
      "types": "./dist/emulators/index.d.ts",
      "default": "./dist/emulators/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint ."
  },
  "peerDependencies": {
    "@angular/core": ">=19.0.0",
    "firebase": ">=11.0.0",
    "rxjs": ">=7.0.0"
  },
  "devDependencies": {
    "@angular/core": "^21.0.6",
    "firebase": "^12.9.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "typescript": "~5.8.0",
    "typescript-eslint": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-unused-imports": "^4.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "ARCHITECTURE.md"
  ],
  "keywords": [
    "angular",
    "firebase",
    "firestore",
    "rxjs",
    "signals"
  ]
}
